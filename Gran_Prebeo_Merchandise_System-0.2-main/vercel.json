{
  "version": 2,
  "functions": {
    "api/server.php": { "runtime": "vercel-php@0.7.2" },
    "api/*.php": { "runtime": "vercel-php@0.7.2" },
    "api/**/*.php": { "runtime": "vercel-php@0.7.2" }
  },
  "routes": [
    { "src": "/(.*\\.(?:css|js|png|jpg|jpeg|gif|svg|ico|webp|woff|woff2|ttf|map))", "dest": "Gran_Prebeo_Merchandise_System/$1" },
    { "src": "/(.*)", "dest": "api/server.php" }
  ]
}
